<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" 
"http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="shop" extends="globalResult">
		<!-- shop : 店铺表-->
		<action name="shop_*" class="com.gtercn.carhome.dealer.cms.action.ShopAction"  method="{1}">
			<result name="list"  >/jsp/shop/list.jsp</result>
			<result name="add"   >/jsp/shop/add.jsp</result>
			<result name="update">/jsp/shop/update.jsp</result>
		</action>
	</package>
</struts>    
