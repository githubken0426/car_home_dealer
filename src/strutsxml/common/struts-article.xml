<struts>
	<package name="article" extends="globalResult">
		<!-- question_and_article : 问题与文章-->
		<action name="article_*" class="com.gtercn.carhome.dealer.cms.action.ArticleAction" method="{1}">
			<result name="list">/jsp/article/list.jsp</result>
