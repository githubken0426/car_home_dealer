<struts>	
	<package name="expertTop" extends="globalResult">
		<action name="expertTop_*" class="com.gtercn.carhome.dealer.cms.action.ExpertTopAction" method="{1}">
			<result name="list">/jsp/expert_top/list.jsp</result>
			<result name="add">/jsp/expert_top/expertAdd.jsp</result>
